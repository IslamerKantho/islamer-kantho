{
  "name": "islamer-kantho",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/client": "^2.8.0",
    "@sanity/image-url": "^0.140.19",
    "antd": "^4.16.8",
    "classnames": "^2.2.6",
    "date-fns": "^2.15.0",
    "formik": "^2.2.9",
    "next": "^10.2.0",
    "next-sanity": "^0.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.3.6",
    "react-icons": "^4.2.0",
    "sass": "^1.36.0",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@sanity/cli": "^2.0.5",
    "autoprefixer": "^10.3.1",
    "lerna": "^4.0.0"
  },
  "description": "This is a demo of how to add a simple comment section to blog post using [Next.js](https://nextjs.org), [Sanity.io](https://www.sanity.io), and [Vercel](https://vercel.com).",
